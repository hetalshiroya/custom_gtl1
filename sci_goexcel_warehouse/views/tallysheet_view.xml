<odoo>

	<!-- Tree View Tally Sheet -->
     <record id='view_tree_warehouse_tally_sheet' model='ir.ui.view'>
        <field name="name">Job Sheet Tree</field>
        <field name="model">warehouse.tally.sheet</field>
        <field name="arch" type="xml">
			<tree string='Warehouse Job Sheet'>
                 <field name="job_no"/>
				 <field name="customer"/>
				 <field name="receipt_date"/>
                 <field name="cargo_type"/>
				 <field name="customer_reference_no"/>
				 <field name="shipment_type" widget="radio" options="{'horizontal': true}"/>
				 <field name="priority" widget="priority"/>
<!--				 <field name="seal_no"/>-->
				 <field name="job_scope"/>
                 <field name="packing_type"/>
                <!-- <field name="sorting_by"/>-->
                <!-- <field name="packing_on_pallet"/>-->
				 <field name="remark"/>
				 <field name="create_date"/>
				 <field name="write_date"/>
			</tree>
		</field>
	</record>

    <!-- View Receipt -->
    <record id="view_picking_list_warehouse_receipt" model="ir.actions.act_window">
        <field name="name">Receipt</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="stock.vpicktree"/>
        <field name="domain">[('tallysheet_reference', '=', active_id), ('picking_type_id','=', 1)]</field>
    </record>

    <!-- View DO -->
    <record id="view_picking_list_warehouse_do" model="ir.actions.act_window">
        <field name="name">Delivery Order</field>
        <field name="res_model">stock.picking</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="stock.vpicktree"/>
        <field name="domain">[('tallysheet_reference', '=', active_id), ('picking_type_id','=', 2)]</field>
    </record>

