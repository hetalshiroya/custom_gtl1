            <calendar color="job_no" date_start="receipt_date" string="Job Sheet" mode="week" quick_add="0"
                      all_day="receipt_date">
                     <field name="customer" />
                     <field name="job_status" />
                    <!-- <field name="shipment_close_date_time" />
                     <field name="port_of_discharge_eta" />-->
            </calendar>
        </field>
    </record>


     <record id="view_tally_sheet_pivot" model="ir.ui.view">
          <field name="name">warehouse.tally.sheet.pivot</field>
         <field name="model">warehouse.tally.sheet</field>
         <field name="arch" type="xml">
             <pivot string="Warehouse Job Sheet Analysis">
                <field name="customer" type="row"/>
                <field name="container_qty" type="measure"/>
                 <field name="do_count" type="measure"/>
                 <field name="receipt_count" type="measure"/>
                 <field name="total_packages" type="measure"/>
                 <field name="receipt_date" interval="month" type="row"/>
                 <!--<field name="port_of_loading_eta" interval="month" type="row"/>-->
             </pivot>
         </field>
    </record>


     <record id="view_tally_sheet_graph" model="ir.ui.view">
         <field name="name">warehouse.tally.sheet.graph</field>
         <field name="model">warehouse.tally.sheet</field>
         <field name="arch" type="xml">
             <graph string="Warehouse Job Sheet Graph" type="line">
                 <field name="customer" type="col"/>
                 <field name="container_qty" type="measure"/>
                 <field name="do_count" type="measure"/>
                 <field name="receipt_count" type="measure"/>
                 <field name="total_packages" type="measure"/>
             </graph>
         </field>
    </record>

