     <!-- Search Tally Sheet -->
    <record id="view_search_tally_sheet" model="ir.ui.view">
       <field name="name">warehouse.tally.sheet.search</field>
        <field name="model">warehouse.tally.sheet</field>
        <field name="arch" type="xml">
            <search>
                <field name="job_no"/>
                <field name="customer"/>
                <filter string="Import" name="import" domain="[('shipment_type', '=', 'import')]"/>
                <filter string="Export" name="export" domain="[('shipment_type', '=', 'export')]"/>
                <filter string="Draft" name="job status" domain="[('job_status', '=', '01')]"/>
                <filter string="Confirmed" name="job status" domain="[('job_status', '=', '02')]"/>
                <filter string="In Progress" name="job status" domain="[('job_status', '=', '05')]"/>
                <filter string="Done" name="job status" domain="[('job_status', '=', '03')]"/>
                <filter string="FCL" name="fcl" domain="[('cargo_type', '=', 'fcl')]"/>
                <filter string="LCL" name="lcl" domain="[('cargo_type', '=', 'lcl')]"/>
                <filter string="Receipt Date" name="filter_receipt_date" date="receipt_date"/>
                <field name="container_line_ids" string="Container No" filter_domain="[('container_line_ids.container_no','ilike',self)]"/>
                <field name="container_line_ids" string="Truck No" filter_domain="[('container_line_ids.truck_no','ilike',self)]"/>
                <field name="container_line_ids" string="SKU" filter_domain="[('container_line_ids.product','ilike',self)]"/>
                <field name="container_line_ids" string="Marking" filter_domain="[('container_line_ids.marking','ilike',self)]"/>

                <group expand="0" string="Group By">
                    <filter string="Receipt Date" name="receipt_month" domain="[]" context="{'group_by':'receipt_date'}"/>
                    <filter string="Salesperson" name="sales_person" domain="[]" context="{'group_by':'sales_person'}"/>
                    <filter string="Owner" name="owner" domain="[]" context="{'group_by':'owner'}"/>
                    <filter string="Customer" name="customer" domain="[]" context="{'group_by':'customer'}"/>
                </group>
            </search>
        </field>
    </record>


     <record id="view_calendar_tally_sheet" model="ir.ui.view">
         <field name="name">warehouse.tally.sheet.calendar</field>
         <field name="model">warehouse.tally.sheet</field>
