<odoo>

 		<record id="gate_pass_view_form" model="ir.ui.view">
            <field name="name">gate.pass.wizard.form</field>
            <field name="model">gate.pass.wizard</field>
            <field name="arch" type="xml">
                <form string="Create Gate Pass">
                    <group class="oe_title">
                        <field name="container_line_gatepass_ids2">
                            <tree editable="bottom">
                                <field name="sequence" widget="handle"/>
                              <!--  <field name="container_no" readonly="1"/>
                                <field name="seal_no" readonly="1"/>-->
                                <field name="time_in"/>
                                <field name="time_out"/>
                                <field name="truck_no"/>
                                <field name="driver"/>
                                <field name="prepared_by"/>
                                <field name="received_by"/>
                                <field name="security"/>
                                <field name="remark_line"/>
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <button name="action_create_gate_pass" string="Create" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

</odoo>